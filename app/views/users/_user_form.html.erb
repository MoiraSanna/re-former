<%= form_for(User.new) do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username, placeholder: "username here" %><br>
  <%= f.label :email %>
  <%= f.text_field :email, placeholder: "example@example.com" %><br>
  <%= f.label :password %>
  <%= f.password_field :password %><br>
  <%= f.submit "Submit" %>
<% end %>  



